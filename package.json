    "jsdom": "^22.1.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.31",
    "sass": "^1.69.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.17.0",
    "vite": "^4.4.11",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.18"

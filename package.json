{
  "name": "vite-vue-typescript-starter",
  "type": "module",
  "version": "0.3.0",
  "private": true,
  "description": "A Vite + Vue 3 + TypeScript starter project with pinia, vue-i18n, tailwindcss, eslint, prettier, and more.",
  "author": "Pascal Achard",
  "scripts": {
    "dev": "vite",
    "build-only": "vite build",
    "build": "run-p type-check \"build-only {@}\" --",

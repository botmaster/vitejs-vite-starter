    "jsdom": "^23.0.1",
    "lodash": "^4.17.21",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.1",
    "vite": "^5.0.10",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.1.1",
    "vue-tsc": "^1.8.27"

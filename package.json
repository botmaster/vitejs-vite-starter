{
  "name": "vite-vue-typescript-starter",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "A Vite + Vue 3 + TypeScript starter project with pinia, vue-i18n, tailwindcss, eslint, prettier, and more.",
  "author": "Pascal Achard",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "sizecheck": "npx vite-bundle-visualizer",
    "test": "vitest"
  },
  "dependencies": {
